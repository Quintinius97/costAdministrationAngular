{
  "extends":"google",

  "plugins":[
    "import"
  ],

  "globals": {
    "angular": 1
  },

  "rules":{
    "linebreak-style":["warn",
      "windows"
    ],
    "func-names":"off",
    "vars-on-top": "off",
    "max-len": "off",
    "quotes": "off",
    "indent": "off",
    "no-undef": "off",
    "spaced-comment": "off",
    "no-var": "off",
    "comma-dangle": "off",
    "new-cap": "off",
    "require-jsdoc": "off"
  }
}